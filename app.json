{
  "expo": {
    "name": "Spot-Hit",
    "slug": "spot-hit-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
	"plugins": [
      "react-native-iap",
      ["expo-build-properties", { "android": { "kotlinVersion": "2.1.20" } }]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.spothit.game"
    },
    "web": {
    }
  }
}
